{
  "name": "@sap/cds-odata-v2-adapter-proxy-test",
  "dependencies": {},
  "private": true,
  "scripts": {
    "hdi": "cf cs hana hdi-shared cds-community-test-hana-cloud -c '{\"database_id\": \"fa8a6cbe-1cdc-4dea-84cf-877c09b0f651\"}'",
    "hdi:sk:create": "cf csk cds-community-test-hana-cloud cds-community-test-hana-cloud-key",
    "hdi:sk:show": "cf service-key cds-community-test-hana-cloud cds-community-test-hana-cloud-key",
    "push": "cd app && cf push",
    "build": "cds build --production",
    "deploy": "cd gen/db && npm start",
    "tunnel": "cd db && npm run tunnel"
  },
  "cds": {
    "requires": {
      "db": {
        "multiTenant": false,
        "kind": "hana",
        "vcap": {
          "tag": "hana"
        }
      }
    },
    "hana": {
      "syntax": "hdi"
    },
    "build": {
      "new-csn": true,
      "tasks": [
        {
          "src": "db",
          "for": "hana",
          "options": {
            "model": [
              "db",
              "srv"
            ]
          }
        },
        {
          "src": "srv",
          "for": "node-cf",
          "options": {
            "model": [
              "db",
              "srv"
            ]
          }
        }
      ]
    }
  }
}
