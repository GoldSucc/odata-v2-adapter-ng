{
  "name": "@sap/cds-odata-v2-adapter-proxy",
  "version": "1.4.39-SNAPSHOT",
  "description": "OData v2 Adapter Proxy for CDS OData v4 Services",
  "homepage": "https://cap.cloud.sap/",
  "engines": {
    "node": ">=8.9.0"
  },
  "author": "Oliver Klemenz, Richard Lindner (SAP SE - https://www.sap.com)",
  "keywords": [
    "CAP",
    "CDS"
  ],
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "XS_APP_LOG_LEVEL=debug cds run --in-memory",
    "cds:run": "npm run start",
    "test": "jest --runInBand --updateSnapshot",
    "test:debug": "XS_APP_LOG_LEVEL=debug jest --runInBand --updateSnapshot",
    "test:integration": "jest --config=jest-integration.config.js",
    "test:server": "XS_APP_LOG_LEVEL=debug node test-server",
    "lint": "npm run prettier && npm run eslint",
    "prettier": "prettier \"**/*.{js,json,md,yml,yaml}\" --write --loglevel error",
    "eslint": "eslint . --fix",
    "upgrade-lock": "rm -rf package-lock.json node_modules && npm i --package-lock=true",
    "test:deploy": "cd test/_env && cds deploy index.cds --to sqlite:test.db --no-save",
    "prepareRelease": "npm prune --production"
  },
  "dependencies": {
    "@sap/logging": "5.3.0",
    "debug": "4.1.1",
    "express": "4.17.1",
    "http-proxy-middleware": "1.0.4",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@sap/cds": "^3.34.2",
    "@sap/hana-client": "^2.5.83",
    "@sap/xssec": "^3.0.2",
    "compression": "^1.7.4",
    "eslint": "^7.1.0",
    "eslint-config": "^0.3.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.5.2",
    "jest-junit": "^10.0.0",
    "passport": "^0.4.1",
    "prettier": "^2.0.5",
    "sqlite3": "4.2.0",
    "supertest": "^4.0.2"
  },
  "license": "SAP DEVELOPER LICENSE 3.1",
  "peerDependencies": {
    "@sap/cds": "*",
    "express": "*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.wdf.sap.corp:cds-community/cds-odata-v2-adapter-proxy.git"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": ":memory:"
        }
      }
    }
  }
}
