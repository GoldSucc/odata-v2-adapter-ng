{
  "name": "@sap/cds-odata-v2-adapter-proxy",
  "version": "1.8.18",
  "description": "CDS OData V2 Adapter Proxy for CDS OData V4 Services",
  "homepage": "https://cap.cloud.sap/",
  "engines": {
    "node": ">=8.9.0"
  },
  "author": "Oliver Klemenz, Richard Lindner (SAP SE - https://www.sap.com)",
  "keywords": [
    "COV2AP",
    "CAP",
    "CDS",
    "ODATA",
    "V2",
    "V4"
  ],
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "XS_APP_LOG_LEVEL=debug cds run --in-memory",
    "cds:run": "npm start",
    "cds:run:single": "XS_APP_LOG_LEVEL=debug cds serve --in-memory --service MainService --from ./test/_env/model --at /",
    "test": "jest --runInBand --updateSnapshot",
    "test:debug": "XS_APP_LOG_LEVEL=debug jest --runInBand --updateSnapshot",
    "test:integration": "jest --config=jest-integration.config.js --updateSnapshot",
    "test:server": "XS_APP_LOG_LEVEL=debug node test-server",
    "test:approuter": "cd approuter && npm start",
    "lint": "npm run prettier && npm run eslint",
    "prettier": "prettier \"**/*.{js,json,md,yml,yaml}\" --write --loglevel error",
    "eslint": "eslint . --fix",
    "upgrade-lock": "rm -rf package-lock.json node_modules && npm i --package-lock=true",
    "test:deploy": "cd test/_env && cds deploy index.cds --to sqlite:test.db --no-save",
    "prepareRelease": "npm prune --production"
  },
  "dependencies": {
    "@sap/logging": "^6.1.1",
    "@types/express": "^4.17.13",
    "express": "^4.18.1",
    "express-fileupload": "^1.3.1",
    "http-proxy-middleware": "^2.0.6",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@sap/cds": "^5.9.5",
    "@sap/hana-client": "^2.13.13",
    "compression": "^1.7.4",
    "eslint": "^8.15.0",
    "eslint-config": "^0.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^28.1.0",
    "jest-junit": "^13.2.0",
    "passport": "^0.5.2",
    "prettier": "^2.6.2",
    "sqlite3": "5.0.8",
    "supertest": "^6.2.3"
  },
  "license": "SEE LICENSE IN LICENSE",
  "cds": {
    "cov2ap": {
      "path": "v2",
      "caseInsensitive": true,
      "fixDraftRequests": true
    },
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": ":memory:"
        }
      }
    }
  }
}
