{
  "name": "@sap/cds-odata-v2-adapter-proxy",
  "version": "1.4.11-SNAPSHOT",
  "description": "OData v2 Adapter Proxy for CDS OData v4 Services",
  "engines": {
    "node": ">=8.9.0"
  },
  "author": "Oliver Klemenz, Richard Lindner",
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "node test-server",
    "test": "jest --updateSnapshot",
    "test:integration": "jest --config=jest-integration.json",
