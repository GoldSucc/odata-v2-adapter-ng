  },
  "devDependencies": {
    "@sap/cds": "^3.20.0",
    "@sap/hana-client": "^2.4.167",
    "@sap/xssec": "^2.2.3",
    "eslint": "^6.6.0",
    "eslint-config": "^0.3.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "prettier": "^1.19.1",
    "sqlite3": "4.0.7",
    "supertest": "^4.0.2"
  },
  "peerDependencies": {
    "@sap/cds": "*",
    "express": "*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.wdf.sap.corp:cds-community/cds-odata-v2-adapter-proxy.git"
