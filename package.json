{
  "name": "@sap/cds-odata-v2-adapter-proxy",
  "version": "1.4.30-SNAPSHOT",
  "description": "OData v2 Adapter Proxy for CDS OData v4 Services",
  "engines": {
    "node": ">=8.9.0"
  },
  "author": "Oliver Klemenz, Richard Lindner",
  "files": [
    "lib/"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "node test-server",
    "cds:run": "cds run",
    "test": "XS_APP_LOG_LEVEL=debug jest --runInBand --updateSnapshot",
    "test:debug": "XS_APP_LOG_LEVEL=debug jest --updateSnapshot",
    "test:mem:install": "npm install --no-save weak-napi",
    "test:mem:heap": "node --expose-gc node_modules/jest/bin/jest --runInBand --logHeapUsage 2>memheap.txt",
    "test:mem:leaks": "jest --runInBand --detectLeaks 2>memleaks.txt",
    "test:integration": "jest --config=jest-integration.config.js",
    "lint": "npm run prettier && npm run eslint",
    "prettier": "prettier \"**/*.{js,json,md,yml,yaml}\" --write --loglevel error",
    "eslint": "eslint . --fix",
    "afterInstall": "npm ci",
    "prepareRelease": "npm prune --production",
    "upgrade-lock": "rm -rf package-lock.json node_modules && npm i --package-lock=true",
    "test:deploy": "cd test/_env && cds deploy index.cds --to sqlite:test.db --no-save"
  },
  "dependencies": {
    "@sap/logging": "^5.3.0",
    "express": "4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@sap/cds": "^3.34.1",
    "@sap/hana-client": "^2.4.196",
    "@sap/xssec": "^2.2.4",
    "eslint": "^6.7.2",
    "eslint-config": "^0.3.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^25.5.2",
    "jest-junit": "^10.0.0",
    "passport": "^0.4.1",
    "prettier": "^2.0.5",
    "sqlite3": "4.0.7",
    "supertest": "^4.0.2"
  },
  "peerDependencies": {
    "@sap/cds": "*",
    "express": "*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.wdf.sap.corp:cds-community/cds-odata-v2-adapter-proxy.git"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
